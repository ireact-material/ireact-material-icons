{
  "name": "ireact-material-icons",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "unpkg": "./dist/index.umd.js",
  "sideEffects": false,
  "repository": "https://github.com/ireact-material/ireact-material-icons",
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "clean": "./scripts/cleanup.sh",
    "start": "dumi dev",
    "lint": "eslint src/ --ext .tsx,.ts",
    "compile": "father build",
    "test": "father test",
    "ci": "NODE_ENV=ci npm run prepublishOnly",
    "test:local": "npm run generate && npm run compile && npm run test",
    "prepublishOnly": "npm run generate && npm run compile && npm run lint && npm run test",
    "generate": "rimraf src/icons && TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/generate.ts"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/classnames": "^2.3.1",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@umijs/fabric": "^3.0.0",
    "cross-env": "^7.0.3",
    "dumi": "1.1.4",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.32.0",
    "father": "^4.1.3",
    "glob": "^8.1.0",
    "lodash": "^4.17.21",
    "mobx": "^6.7.0",
    "mobx-react": "^7.6.0",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.0.7",
    "styled-components": "^5.3.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "classnames": "^2.3.2",
    "eslint-plugin-react": "^7.32.0",
    "ireact-material-colors": "^0.0.1",
    "ireact-material-icon-svg": "^0.0.1",
    "rc-util": "^5.27.1",
    "react": "^18.2.0"
  },
  "resolutions": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1"
  }
